<!-- DataTales Example -->
<div class="tab-pane fade" id="show-image-feature" role="tabpanel" aria-labelledby="show-item-tab">
    <div class="row mt-2">
        <% for(let x = 0; x < feature.length; x++) {%>
        <div class="col-12 col-sm-12 col-md-4 col-lg-4">
            <div class="card shadow mb-3">
                <div class="card-body">
                    <img src="<%= baseURL+feature[x].imageUrl  %>" alt="image" class="w-100 mb-2" />
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-dark">Back</a>
</div>